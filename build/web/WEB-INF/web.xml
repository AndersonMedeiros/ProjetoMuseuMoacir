<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>cadastro_usuario</servlet-name>
        <servlet-class>Servlet.cadastro_usuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Pre_Cadastro_Usuario</servlet-name>
        <servlet-class>Servlet.Pre_Cadastro_Usuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cadastro_obras</servlet-name>
        <servlet-class>Servlet.cadastro_obras</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Agendamento_visita</servlet-name>
        <servlet-class>Servlet.Agendamento_visita</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Entrar</servlet-name>
        <servlet-class>Servlet.Entrar</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>cadastro_usuario</servlet-name>
        <url-pattern>/cadastro_usuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Pre_Cadastro_Usuario</servlet-name>
        <url-pattern>/Pre_Cadastro_Usuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cadastro_obras</servlet-name>
        <url-pattern>/cadastro_obras</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Agendamento_visita</servlet-name>
        <url-pattern>/Agendamento_visita</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Entrar</servlet-name>
        <url-pattern>/Entrar</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
